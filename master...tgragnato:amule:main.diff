-/// Convert hash to hexa string
-wxString MD4::charToHex(const char *buf, size_t len)
-{
-  size_t i;
-  wxString hexString;
-
-  for (i = 0; i < len; ++i)
-    {
-      hexString += wxString::Format(wxS("%02x"), 0xFF & *(buf + i));
-    }
-
-  // Reduce memory usage
-  hexString.Shrink();
-
-  return (hexString);
-}
