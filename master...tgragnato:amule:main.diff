-bool
-WxCasPrint::OnPrintPage ( int page )
-{
-	wxDC * dc = GetDC ();
-	if ( dc ) {
-		if ( page == 1 ) {
-			DrawPageOne ( dc );
-		}
-
-
-		dc->SetDeviceOrigin ( 0, 0 );
-		dc->SetUserScale ( 1.0, 1.0 );
