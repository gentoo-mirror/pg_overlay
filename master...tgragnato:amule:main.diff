-m4_define([MULE_IF],
-[m4_if( [$#], 0,,
-	[$#], 1,,
-	[m4_n([if $1; then])  m4_ifval([$2],[$2], :)
-m4_if(m4_eval([$# > 2]), 1, [__mule_if_helper(m4_shiftn(2, $@))])m4_n([fi])])])
